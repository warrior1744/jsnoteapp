itor from \"@uiw/react-md-editor\";\r\nimport { Cell } from \"../state\";\r\nimport { useTypedSelector } from \"../hooks/useTypedSelector\";\r\nimport { useActions } from \"../hooks/useActions\";\r\n\r\ninterface TextEditorProps {\r\n  cell: Cell;\r\n}\r\n\r\nconst TextEditor = ({ cell }: TextEditorProps) => {\r\n  const [editing, setEditing] = useState(false);\r\n  const editorRef = useRef<HTMLDivElement | null>(null);\r\n\r\n  const { updateCell } = useActions();\r\n\r\n  useEffect(() => {\r\n    const listener = (event: MouseEvent) => {\r\n      if (\r\n        editorRef.current &&\r\n        event.target &&\r\n        editorRef.current.contains(event.target as Node)\r\n      ) {\r\n        //if click event target is any of the Ref DOM element tree\r\n        return;\r\n      }\r\n      setEditing(false);\r\n    };\r\n\r\n    document.addEventListener(\"click\", listener, { capture: true });\r\n\r\n    return () => {\r\n      document.removeEventListener(\"click\", listener, { capture: true });\r\n    };\r\n  }, []);\r\n\r\n  if (editing) {\r\n    return (\r\n      <div className=\"text-editor\" ref={editorRef}>\r\n        <MDEditor\r\n          value={cell.content}\r\n          onChange={(v) => updateCell(cell.id, v || \"\")}\r\n        />\r\n      </div>\r\n    );\r\n  }\r\n\r\n  //editing is false\r\n  return (\r\n    <div className=\"text-editor card\" onClick={() => setEditing(true)}>\r\n      <div className=\"card-content\">\r\n        <MDEditor.Markdown source={cell.content || \"Click to edit\"} />\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default TextEditor;\r\n","C:\\Users\\user\\Dropbox\\Dev\\Typescript\\react-ts-portfolio\\jsnote\\packages\\local-client\\src\\components\\CodeCell.tsx",["115"],"import \"./styling/code-cell.css\";\r\nimport { useEffect } from \"react\";\r\nimport CodeEditor from \"./CodeEditor\";\r\nimport Preview from \"./Preview\";\r\nimport Resizable from \"./Resizable\";\r\nimport { Cell } from \"../state\";\r\nimport